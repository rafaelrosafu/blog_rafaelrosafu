---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	<title>{{site.name}}</title>
    <subtitle>{{site.feed_subtitle}}</subtitle>
    <link href="{{site.production_url}}/atom.xml" type="application/atom+xml" rel="self" />
    <link href="{{site.production_url}}" />
    <id>{{site.production_url}}/</id>
    <updated>{{ site.time | date_to_xmlschema }}</updated>
   	<author>
    	<name>{{site.author.name}}</name>
    	<email>{{site.author.email}}</email>
  	</author>
	{% for post in site.posts %}
    <entry>
		<id>{{site.production_url}}{{ post.id }}</id>
		<link type="text/html" rel="alternate" href="http://functionn.github.com{{ post.url }}"/>
	    <title>{{ post.title }}</title>
	    <published>{{ post.date | date_to_xmlschema }}</published>
	    <updated>{{ post.date | date_to_xmlschema }}</updated>
	    <author>
	    	<name>{{site.author.name}}</name>
	    	<email>{{site.author.email}}</email>
	    </author>
	    <summary>{{ post.content | strip_html | xml_escape }}</summary>
    </entry>
	{% endfor %}
</feed>